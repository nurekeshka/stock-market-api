{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "declaration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "module": "commonjs",
    "noFallthroughCasesInSwitch": false,
    "noImplicitAny": false,
    "outDir": "./dist",
    "paths": {
      "@libs/common": [
        "libs/common/src"
      ],
      "@libs/configuration": [
        "libs/configuration/src"
      ],
      "@libs/database": [
        "libs/database/src"
      ],
      "@libs/finnhub": [
        "libs/finnhub/src"
      ]
    },
    "removeComments": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strictBindCallApply": false,
    "strictNullChecks": true,
    "target": "ES2021"
  }
}